<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,cq.siteadmin.admin.properties]"
    mode="edit">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="cq-dialog-content-page">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container"
                rel="cq-siteadmin-admin-properties-tabs">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    type="nav"/>
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Title and Tags"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pagename
                                                cq:hideOnEdit="{Boolean}true"
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                fieldLabel="Name"
                                                name="pageName"
                                                validation="foundation.jcr.name"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                cq-msm-lockable="jcr:title"
                                                fieldLabel="Title*"
                                                name="./jcr:title"
                                                renderReadOnly="{Boolean}true"
                                                required="{Boolean}true"/>
                                            <msmConfigEndpoint
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                class="msm-config-endpoint"
                                                value="${empty param.item ? requestPathInfo.suffix : param.item }"/>
                                            <tags
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/common/tagspicker"
                                                allowBulkEdit="{Boolean}true"
                                                allowCreate="{Boolean}true"
                                                cq-msm-lockable="cq:tags"
                                                fieldLabel="Tags"
                                                name="./cq:tags"
                                                renderReadOnly="{Boolean}true"/>
                                            <hideinnav
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="hideInNav"
                                                name="./hideInNav"
                                                renderReadOnly="{Boolean}true"
                                                text="Hide in Navigation"
                                                value="true"/>
                                            <deletehideinnav
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                name="./hideInNav@Delete"
                                                value="true"/>
                                            <charset
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                allowBulkEdit="{Boolean}true"
                                                ignoreData="{Boolean}true"
                                                name="_charset_"
                                                value="utf-8"/>
                                        </items>
                                    </title>
                                    <moretitles
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="More Titles and Description"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pagetitle
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="pageTitle"
                                                fieldLabel="Page Title"
                                                name="./pageTitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <navigationtitle
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="navTitle"
                                                fieldLabel="Navigation Title"
                                                name="./navTitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <subtitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="subtitle"
                                                fieldLabel="Subtitle"
                                                name="./subtitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="jcr:description"
                                                fieldLabel="Description"
                                                name="./jcr:description"
                                                renderReadOnly="{Boolean}true"/>
                                        </items>
                                    </moretitles>
                                    <onofftime
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="On/Off Time"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ondate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/datepicker"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="onTime"
                                                displayedFormat="MMMM DD, YYYY hh:mm a"
                                                displayTimezoneMessage="{Boolean}true"
                                                fieldLabel="On Time"
                                                name="./onTime"
                                                renderReadOnly="{Boolean}true"
                                                type="datetime"/>
                                            <ondate_type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                allowBulkEdit="{Boolean}true"
                                                ignoreData="{Boolean}true"
                                                name="./onTime@TypeHint"
                                                value="Date"/>
                                            <offdate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/datepicker"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="offTime"
                                                displayedFormat="MMMM DD, YYYY hh:mm a"
                                                displayTimezoneMessage="{Boolean}true"
                                                fieldLabel="Off Time"
                                                name="./offTime"
                                                renderReadOnly="{Boolean}true"
                                                type="datetime"/>
                                            <offdate_type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                allowBulkEdit="{Boolean}true"
                                                ignoreData="{Boolean}true"
                                                name="./offTime@TypeHint"
                                                value="Date"/>
                                        </items>
                                    </onofftime>
                                    <vanityurl
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Vanity URL"
                                        sling:hideResource="{Boolean}true"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <vanitypath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                cq-msm-lockable="sling:vanityPath"
                                                fieldLabel="Vanity URL"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    name="./sling:vanityPath"
                                                    required="{Boolean}true"/>
                                            </vanitypath>
                                            <redirectVanityURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                cq-msm-lockable="./sling:redirect"
                                                name="./sling:redirect"
                                                renderReadOnly="{Boolean}true"
                                                text="Redirect Vanity URL"
                                                value="true"/>
                                        </items>
                                    </vanityurl>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <seo
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO Settings"
                        sling:orderBefore="thumbnail"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <canonicalTag
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                        class="canonical-tag"
                                        fieldLabel="Canonical Tag"
                                        name="./canonicalTag"
                                        renderReadOnly="{Boolean}true"/>
                                    <hideinnav
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="hideInNav"
                                        fieldDescription="By clicking this checkbox you can hide this page from web crawlers. This is also referred as 'noindex' tag"
                                        name="./hideInNav"
                                        renderReadOnly="{Boolean}true"
                                        text="Hide in SiteMap"
                                        value="true"/>
                                    <changefreq
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="changeFreq"
                                        fieldDescription="This value provides general information to search engines and may not correlate exactly to how often they crawl the page. Valid values are daily, weekly and monthly"
                                        fieldLabel="Change Frequency for the page"
                                        name="./changeFreq"
                                        renderReadOnly="{Boolean}true">
                                        <items jcr:primaryType="cq:WidgetCollection">
                                            <never
                                                jcr:primaryType="nt:unstructured"
                                                text="Never"
                                                value="never"/>
                                            <yearly
                                                jcr:primaryType="nt:unstructured"
                                                text="Yearly"
                                                value="yearly"/>
                                            <monthly
                                                jcr:primaryType="nt:unstructured"
                                                text="Monthly"
                                                value="monthly"/>
                                            <weekly
                                                jcr:primaryType="nt:unstructured"
                                                selected="{Boolean}true"
                                                text="Weekly"
                                                value="weekly"/>
                                            <daily
                                                jcr:primaryType="nt:unstructured"
                                                text="Daily"
                                                value="daily"/>
                                            <hourly
                                                jcr:primaryType="nt:unstructured"
                                                text="Hourly"
                                                value="hourly"/>
                                            <always
                                                jcr:primaryType="nt:unstructured"
                                                text="Always"
                                                value="always"/>
                                        </items>
                                    </changefreq>
                                    <priorityprop
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="priorityProp"
                                        fieldDescription="The priority of this URL relative to other URLs on your site. Valid values range from 0.0 to 1.0."
                                        fieldLabel="Priority of Page"
                                        name="./priority"
                                        renderReadOnly="{Boolean}true"
                                        value="0.5"/>
                                    <robots
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        allowBulkEdit="{Boolean}true"
                                        cq-msm-lockable="robots"
                                        fieldLabel="Robots tags"
                                        multiple="{Boolean}true"
                                        name="./robots"
                                        renderReadOnly="{Boolean}true">
                                        <items jcr:primaryType="cq:WidgetCollection">
                                            <select
                                                jcr:primaryType="nt:unstructured"
                                                text=""
                                                value=""/>
                                            <index
                                                jcr:primaryType="nt:unstructured"
                                                text="Index"
                                                value="index"/>
                                            <noindex
                                                jcr:primaryType="nt:unstructured"
                                                text="No Index"
                                                value="noindex"/>
                                            <follow
                                                jcr:primaryType="nt:unstructured"
                                                text="Follow"
                                                value="follow"/>
                                            <nofollow
                                                jcr:primaryType="nt:unstructured"
                                                text="No Follow"
                                                value="nofollow"/>
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value="none"/>
                                            <noarchive
                                                jcr:primaryType="nt:unstructured"
                                                text="No Archive"
                                                value="noarchive"/>
                                            <nosnippet
                                                jcr:primaryType="nt:unstructured"
                                                text="No Snippet"
                                                value="nosnippet"/>
                                            <noodp
                                                jcr:primaryType="nt:unstructured"
                                                text="No Odp"
                                                value="noodp"/>
                                            <noydir
                                                jcr:primaryType="nt:unstructured"
                                                text="No Ydir"
                                                value="noydir"/>
                                        </items>
                                    </robots>
                                    <vanityurl
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Vanity URL"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <vanitypath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                cq-msm-lockable="sling:vanityPath"
                                                fieldLabel="Vanity URL"
                                                renderReadOnly="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    name="./sling:vanityPath"
                                                    required="{Boolean}true"/>
                                            </vanitypath>
                                            <redirectVanityURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                cq-msm-lockable="./sling:redirect"
                                                name="./sling:redirect"
                                                renderReadOnly="{Boolean}true"
                                                text="Redirect Vanity URL"
                                                value="true"/>
                                        </items>
                                    </vanityurl>
                                    <openGraphTags
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Open Graph Tags"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ogTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="ogTitle"
                                                fieldDescription="The title of your page, content, object etc. as you would like for it to appear when displayed on Facebook."
                                                fieldLabel="Title"
                                                name="./ogTitle"
                                                renderReadOnly="{Boolean}true"/>
                                            <ogSiteName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="ogSiteName"
                                                fieldDescription="The name of your site."
                                                fieldLabel="Sitename"
                                                name="./ogSiteName"
                                                renderReadOnly="{Boolean}true"/>
                                            <ogDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="ogDescription"
                                                fieldDescription="This sentence snippet shows up in the post."
                                                fieldLabel="Description"
                                                name="./ogDescription"
                                                renderReadOnly="{Boolean}true"/>
                                            <ogType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="ogType"
                                                fieldDescription="The type of content."
                                                fieldLabel="Type "
                                                name="./ogType"
                                                renderReadOnly="{Boolean}true"/>
                                            <ogImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="ogImage"
                                                fieldDescription="The URL for an image you want to represent the your content. Images must be either PNG, JPEG and GIF formats and at least 50px by 50px."
                                                fieldLabel="Image "
                                                name="./ogImage"
                                                renderReadOnly="{Boolean}true"
                                                rootPath="/content/dam/"/>
                                            <ogUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="ogURL"
                                                fieldDescription="This will be the URL that will be associated with your content i.e. the link."
                                                fieldLabel="URL"
                                                name="./ogURL"
                                                renderReadOnly="{Boolean}true"
                                                rootPath="/content/cen"/>
                                            <ogLocale
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                allowBulkEdit="{Boolean}true"
                                                cq-msm-lockable="ogType"
                                                fieldDescription="The locale these tags are marked up in. Of the format language_TERRITORY."
                                                fieldLabel="Locale"
                                                name="./ogLocale"
                                                renderReadOnly="{Boolean}true"/>
                                        </items>
                                    </openGraphTags>
                                    <metaTags
                                        cq:showOnCreate="{Boolean}false"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Meta Tags"
                                        sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <metags
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                class="foundation-layout-util-maximized-alt long-label"
                                                fieldDescription="meta name 'viewport' is already present with content as 'width=device-width, initial-scale=1.0'"
                                                fieldLabel="Meta Tags">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                    acs-commons-nested=""
                                                    name="./metaTags">
                                                    <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                        method="absolute"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <name
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                    fieldLabel="Name*"
                                                                    name="./metaName"
                                                                    required="{Boolean}true"/>
                                                                <content
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                    fieldLabel="Content*"
                                                                    name="./metaContent"
                                                                    required="{Boolean}true"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </metags>
                                            <seoKeywords
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                class="foundation-layout-util-maximized-alt long-label"
                                                fieldDescription="Enter SEO keywords seperated by ',' in lowercase Example: article, magazine, blog"
                                                fieldLabel="SEO Keywords"
                                                name="./seoKeywords"/>
                                        </items>
                                    </metaTags>
                                </items>
                            </column>
                        </items>
                    </seo>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
