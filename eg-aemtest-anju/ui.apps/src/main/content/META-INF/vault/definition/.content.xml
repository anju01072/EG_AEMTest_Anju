    jcr:primaryType="vlt:PackageDefinition"
    sling:resourceType="cq/packaging/components/pack/definition"
    buildCount="1"
    cqVersion="6.1"
    group="aemtest"
    name="eg-aemtest"
    path="/etc/packages/aemtest/eg-aemtest-y"
