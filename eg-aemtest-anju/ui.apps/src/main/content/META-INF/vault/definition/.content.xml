            rules="[]"/>



        <f3
            jcr:primaryType="nt:unstructured"
            mode="replace"
            root="/etc/clientlibs/aemtest"
            rules="[]"/>


        <f4
            jcr:primaryType="nt:unstructured"
            mode="replace"
            root="/etc/designs/aemtest"

            rules="[include:/etc/designs/aemtest/jcr:content,include:/etc/designs/aemtest/jcr:content/clientlibs,include:/etc/designs/aemtest/jcr:content/clientlibs/.*]"/>

